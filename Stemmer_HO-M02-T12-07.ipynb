   "source": [
    "#Import the relevant packages\n",
    "from nltk import stem"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "6c6b6749",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Create the function to stem the words \n",
    "def get_stems(word):\n",
    "    stemmer = stem.PorterStemmer()\n",
    "    return stemmer.stem(word)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "03b1e0bd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "come\n"
     ]
    }
   ],
   "source": [
    "output = get_stems('coming')\n",
    "print(output)"
   ]
